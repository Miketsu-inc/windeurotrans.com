---
import Layout from '../layouts/Layout.astro';
import Nav from '../components/Nav.astro';
import Footer from '../components/Footer.astro';

// Welcome to Astro! Wondering what to do next? Check out the Astro documentation at https://docs.astro.build
// Don't want to use any of this? Delete everything in this file, the `assets`, `components`, and `layouts` directories, and start fresh.
---

<Layout>
	<Nav lang="en" />

	<header
      class="bg-position[70%_30%] m-auto flex h-screen w-full items-center
        overflow-hidden bg-[url(/windeurotrans.com/image_4.jpg)] bg-cover bg-center
        bg-no-repeat"
    >
      <div
        class="relative z-10 mx-auto w-full max-w-375 px-6 pb-24 md:px-12
          md:pb-32"
      >
        <span
          class="text-primary mb-4 block text-sm font-bold tracking-[0.2em]
            uppercase md:text-base"
        >
          Logistics Solutions Hungary
        </span>
        <h1
          class="mb-8 max-w-4xl text-5xl leading-none font-bold md:text-7xl
            lg:text-8xl"
        >
          Logistics<br />beyond <br />borders.
        </h1>
        <button
          class="group bg-primary relative cursor-pointer overflow-hidden
            rounded-lg px-8 py-4 text-sm font-semibold tracking-wider text-white
            uppercase transition-all hover:bg-[#e04d0b]"
        >
          Get a Quote
        </button>
      </div>
    </header>
    <section id="whyus" class="relative px-6 pt-24 pb-12 md:px-12 md:py-32">
      <div
        class="absolute right-1/2 bottom-0 left-1/2 -z-10 hidden size-200
          opacity-30 blur-3xl filter md:block"
      >
        <div
          class="from-primary h-full w-full rounded-full bg-linear-to-tr
            to-teal-500"
        ></div>
      </div>

      <div class="mx-auto max-w-7xl">
        <div class="mb-16 md:text-center">
          <h2 class="text-text_color text-4xl font-bold md:text-5xl">
            Why industry leaders choose us.
          </h2>
        </div>

        <div class="grid grid-cols-1 gap-6 md:grid-cols-3 md:grid-rows-2">
          <div
            class="group hover:border-primary/10 relative col-span-1
              overflow-hidden rounded-3xl border border-white/40 bg-white/40 p-8
              shadow-md backdrop-blur-xs transition-all duration-500
              hover:-translate-y-1 hover:bg-white/60 md:col-span-2"
          >
            <div
              class="relative z-10 flex h-full flex-col justify-between gap-8
                md:flex-row"
            >
              <div class="max-w-md space-y-4">
                <div class="flex flex-col gap-6 md:flex-row">
                  <div
                    class="flex size-14 shrink-0 items-center justify-center
                      rounded-2xl bg-teal-800/10 text-teal-800 transition-colors
                      duration-300 group-hover:bg-teal-800
                      group-hover:text-white"
                  >
                    <svg
                      class="size-7"
                      xmlns="http://www.w3.org/2000/svg"
                      width="24"
                      height="24"
                      viewBox="0 0 24 24"
                      fill="none"
                      stroke="currentColor"
                      stroke-width="2"
                      stroke-linecap="round"
                      stroke-linejoin="round"
                    >
                      <path
                        d="M12 22s8-4 8-10V5l-8-3-8 3v7c0 6 8 10 8 10"
                      ></path>
                      <path d="m9 12 2 2 4-4"></path>
                    </svg>
                  </div>
                  <div class="flex flex-col items-start gap-3">
                    <h3 class="text-2xl font-bold">Certified Excellence</h3>
                    <p class="leading-relaxed text-gray-600">
                      We don't just promise quality; we prove it. Our operations
                      are rigorously audited to meet the highest international
                      safety and pharmaceutical standards.
                    </p>
                    <a class="cursor-pointer underline">View certificates</a>
                  </div>
                </div>
              </div>
              <img
                src="combined_certificates.png"
                alt="ISO 9001 & ISO 14001 & GDP"
                class="size-48 w-auto object-contain opacity-80 transition-all
                  duration-300 group-hover:opacity-100 group-hover:grayscale-0
                  md:grayscale"
              />
            </div>
          </div>

          <div
            class="group hover:border-primary/10 relative overflow-hidden
              rounded-3xl border border-white/40 bg-white/40 p-8 shadow-md
              backdrop-blur-xs transition-all duration-500 hover:-translate-y-1
              hover:bg-white/60"
          >
            <div
              class="mb-6 flex size-14 items-center justify-center rounded-2xl
                bg-teal-800/10 text-teal-800 transition-colors duration-300
                group-hover:bg-teal-800 group-hover:text-white"
            >
              <svg
                class="size-7"
                fill="none"
                stroke="currentColor"
                viewBox="0 0 24 24"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M13 10V3L4 14h7v7l9-11h-7z"
                />
              </svg>
            </div>
            <h3 class="mb-3 text-xl font-bold">Time-Critical Speed</h3>
            <p class="text-gray-600">
              Dual-driver teams mean your cargo never stops moving. We
              specialize in "just-in-time" deliveries across the continent.
            </p>
          </div>

          <div
            class="group hover:border-primary/10 relative overflow-hidden
              rounded-3xl border border-white/40 bg-white/40 p-8 shadow-md
              backdrop-blur-xs transition-all duration-500 hover:-translate-y-1
              hover:bg-white/60"
          >
            <div
              class="mb-6 flex size-14 items-center justify-center rounded-2xl
                bg-teal-800/10 text-teal-800 transition-colors duration-300
                group-hover:bg-teal-800 group-hover:text-white"
            >
              <svg
                class="size-7"
                fill="none"
                stroke="currentColor"
                viewBox="0 0 24 24"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M3.055 11H5a2 2 0 012 2v1a2 2 0 002 2 2 2 0 012 2v2.945M8 3.935V5.5A2.5 2.5 0 0010.5 8h.5a2 2 0 012 2 2 2 0 104 0 2 2 0 012-2h1.064M15 20.488V18a2 2 0 012-2h3.064M21 12a9 9 0 11-18 0 9 9 0 0118 0z"
                />
              </svg>
            </div>
            <h3 class="mb-3 text-xl font-bold">Budapest Hub</h3>
            <p class="text-gray-600">
              Ideally situated to bridge Eastern and Western Europe, offering
              optimized routes that save fuel and time.
            </p>
          </div>

          <div
            class="group hover:border-primary/10 relative col-span-1
              overflow-hidden rounded-3xl border border-white/40 bg-white/40 p-8
              shadow-md backdrop-blur-xs transition-all duration-500
              hover:-translate-y-1 hover:bg-white/60 md:col-span-2"
          >
            <div class="flex flex-col gap-6 sm:flex-row sm:items-start">
              <div
                class="mb-3 flex size-14 shrink-0 items-center justify-center
                  rounded-2xl bg-teal-800/10 text-teal-800 transition-colors
                  duration-300 group-hover:bg-teal-800 group-hover:text-white"
              >
                <svg
                  class="size-7"
                  fill="none"
                  stroke="currentColor"
                  viewBox="0 0 24 24"
                >
                  <path
                    stroke-linecap="round"
                    stroke-linejoin="round"
                    stroke-width="2"
                    d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"
                  />
                </svg>
              </div>
              <div>
                <h3 class="mb-3 text-xl font-bold">
                  Modern & Sustainable Fleet
                </h3>
                <p class="max-w-2xl text-gray-600">
                  We operate exclusively with Euro 6 compliant vehicles. This
                  ensures reliability for your schedule and responsibility for
                  our environment. Our commitment to sustainability reduces your
                  supply chain's carbon footprint.
                </p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <section id="services" class="relative px-6 py-12 md:px-12 md:py-32">
      <div
        class="absolute bottom-0 left-0 -z-10 hidden size-150 -translate-x-1/3
          translate-y-1/3 opacity-30 blur-3xl filter md:block"
      >
        <div
          class="to-primary h-full w-full rounded-full bg-linear-to-tr
            from-teal-500"
        ></div>
      </div>

      <div class="mx-auto grid max-w-7xl grid-cols-1 gap-12 lg:grid-cols-2">
        <div class="flex flex-col justify-center lg:sticky lg:top-62 lg:h-fit">
          <h2
            class="text-brand-dark mb-6 text-4xl leading-tight font-bold
              md:text-6xl"
          >
            We move what<br />
            others can't.
          </h2>
          <p class="text-brand-grey text-lg leading-relaxed lg:max-w-md">
            The supply chain is complex; our solutions are simple. From single
            pallets to full industrial convoys, we provide the infrastructure
            your business needs to scale.
          </p>

          <div class="mt-8 hidden lg:block">
            <a
              href="#contact"
              class="group hover:text-primary flex items-center gap-2
                font-semibold text-teal-800 transition-colors"
            >
              Get a quote
              <svg
                class="size-5 transition-transform group-hover:translate-x-1"
                fill="none"
                stroke="currentColor"
                viewBox="0 0 24 24"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M17 8l4 4m0 0l-4 4m4-4H3"
                />
              </svg>
            </a>
          </div>
        </div>

        <div class="flex flex-col gap-2">
          <div
            class="group relative rounded-2xl p-6 transition-all duration-300
              hover:bg-white/40 hover:shadow-lg hover:backdrop-blur-md"
          >
            <div class="flex gap-6 sm:items-start">
              <div
                class="flex size-12 shrink-0 items-center justify-center
                  rounded-xl bg-teal-800/10 text-teal-800 transition-colors
                  duration-300 group-hover:bg-teal-800 group-hover:text-white"
              >
                <svg
                  class="size-6"
                  fill="none"
                  stroke="currentColor"
                  viewBox="0 0 24 24"
                >
                  <path
                    stroke-linecap="round"
                    stroke-linejoin="round"
                    stroke-width="2"
                    d="M13 16V6a1 1 0 00-1-1H4a1 1 0 00-1 1v10a1 1 0 001 1h1m8-1a1 1 0 01-1 1H9m4-1V8a1 1 0 011-1h2.586a1 1 0 01.707.293l3.414 3.414a1 1 0 01.293.707V16a1 1 0 01-1 1h-1m-6-1a1 1 0 001 1h1M5 17a2 2 0 104 0m-4 0a2 2 0 114 0m6 0a2 2 0 104 0m-4 0a2 2 0 114 0"
                  />
                </svg>
              </div>
              <div>
                <h3
                  class="text-brand-dark mb-2 text-xl font-bold
                    transition-colors duration-300 group-hover:text-teal-900"
                >
                  Road Freight (FTL/LTL)
                </h3>
                <p class="text-gray-600">
                  Reliable Full Truck Load and Less than Truck Load services. We
                  optimize routes to ensure cost-efficiency without compromising
                  on speed.
                </p>
              </div>
            </div>
            <div
              class="absolute right-6 bottom-0 left-20 h-px bg-stone-500/10
                transition-opacity duration-300 group-hover:opacity-0"
            ></div>
          </div>

          <div
            class="group relative rounded-2xl p-6 transition-all duration-300
              hover:bg-white/40 hover:shadow-lg hover:backdrop-blur-md"
          >
            <div class="flex gap-6 sm:items-start">
              <div
                class="flex size-12 shrink-0 items-center justify-center
                  rounded-xl bg-teal-800/10 text-teal-800 transition-colors
                  duration-300 group-hover:bg-teal-800 group-hover:text-white"
              >
                <svg
                  class="size-6"
                  fill="none"
                  stroke="currentColor"
                  viewBox="0 0 24 24"
                >
                  <path
                    stroke-linecap="round"
                    stroke-linejoin="round"
                    stroke-width="2"
                    d="M19.428 15.428a2 2 0 00-1.022-.547l-2.384-.477a6 6 0 00-3.86.517l-.318.158a6 6 0 01-3.86.517L6.05 15.21a2 2 0 00-1.806.547M8 4h8l-1 1v5.172a2 2 0 00.586 1.414l5 5c1.26 1.26.367 3.414-1.415 3.414H4.828c-1.782 0-2.674-2.154-1.414-3.414l5-5A2 2 0 009 10.172V5L8 4z"
                  />
                </svg>
              </div>
              <div>
                <h3
                  class="text-brand-dark mb-2 text-xl font-bold
                    transition-colors duration-300 group-hover:text-teal-900"
                >
                  Pharma & Refrigerated
                </h3>
                <p class="text-gray-600">
                  GDP-compliant transport. Our fleet maintains precise
                  temperature control (-25°C to +25°C) with real-time
                  monitoring.
                </p>
              </div>
            </div>
            <div
              class="absolute right-6 bottom-0 left-20 h-px bg-stone-500/10
                transition-opacity duration-300 group-hover:opacity-0"
            ></div>
          </div>

          <div
            class="group relative rounded-2xl p-6 transition-all duration-300
              hover:bg-white/40 hover:shadow-lg hover:backdrop-blur-md"
          >
            <div class="flex gap-6 sm:items-start">
              <div
                class="flex size-12 shrink-0 items-center justify-center
                  rounded-xl bg-teal-800/10 text-teal-800 transition-colors
                  duration-300 group-hover:bg-teal-800 group-hover:text-white"
              >
                <svg
                  class="size-6"
                  fill="none"
                  stroke="currentColor"
                  viewBox="0 0 24 24"
                >
                  <path
                    stroke-linecap="round"
                    stroke-linejoin="round"
                    stroke-width="2"
                    d="M19 11H5m14 0a2 2 0 012 2v6a2 2 0 01-2 2H5a2 2 0 01-2-2v-6a2 2 0 012-2m14 0V9a2 2 0 00-2-2M5 11V9a2 2 0 012-2m0 0V5a2 2 0 012-2h6a2 2 0 012 2v2M7 7h10"
                  />
                </svg>
              </div>
              <div>
                <h3
                  class="text-brand-dark mb-2 text-xl font-bold
                    transition-colors duration-300 group-hover:text-teal-900"
                >
                  Warehousing
                </h3>
                <p class="text-gray-600">
                  Strategic storage solutions in our Budapest hub. Short-term
                  and long-term warehousing with cross-docking capabilities.
                </p>
              </div>
            </div>
            <div
              class="absolute right-6 bottom-0 left-20 h-px bg-stone-500/10
                transition-opacity duration-300 group-hover:opacity-0"
            ></div>
          </div>

          <div
            class="group relative rounded-2xl p-6 transition-all duration-300
              hover:bg-white/40 hover:shadow-lg hover:backdrop-blur-md"
          >
            <div class="flex gap-6 sm:items-start">
              <div
                class="flex size-12 shrink-0 items-center justify-center
                  rounded-xl bg-teal-800/10 text-teal-800 transition-colors
                  duration-300 group-hover:bg-teal-800 group-hover:text-white"
              >
                <svg
                  class="size-6"
                  fill="none"
                  stroke="currentColor"
                  viewBox="0 0 24 24"
                >
                  <path
                    stroke-linecap="round"
                    stroke-linejoin="round"
                    stroke-width="2"
                    d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"
                  />
                </svg>
              </div>
              <div>
                <h3
                  class="text-brand-dark mb-2 text-xl font-bold
                    transition-colors duration-300 group-hover:text-teal-900"
                >
                  Customs Clearance
                </h3>
                <p class="text-gray-600">
                  Navigating post-Brexit and international borders effortlessly
                  with our in-house customs brokerage team.
                </p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <section id="about" class="mx-auto max-w-7xl px-6 py-12 md:px-0 md:py-32">
      <div
        class="grid grid-cols-1 items-center gap-16 md:grid-cols-2 md:gap-24"
      >
        <div>
          <h2
            class="text-brand-dark mb-6 text-4xl leading-tight font-bold
              md:text-5xl"
          >
            Driving the future of European transport.
          </h2>
          <p class="text-brand-grey mb-8 text-lg leading-relaxed">
            Windeurotrans isn't just a trucking company; we are the pulse of
            supply chains connecting Hungary to the global market. With a modern
            fleet and English-speaking logistics experts, we simplify the
            complex.
          </p>
          <a
            href="#"
            class="text-brand-dark border-primary hover:text-primary
              inline-block border-b-2 pb-1 font-bold transition-colors"
          >
            Read Our Story
          </a>
        </div>

        <div class="relative delay-200">
          <img
            src="portrait.jpg"
            alt="Modern Truck"
            class="h-125 w-full rounded-xl object-cover shadow-2xl"
          />

          <div
            class="border-primary absolute -bottom-6 -left-2 max-w-70 rounded-lg
              border-l-4 bg-white/40 p-8 shadow-2xl backdrop-blur-xs
              md:-left-12"
          >
            <strong class="text-brand-dark mb-2 block text-lg"
              >HQ: Budapest</strong
            >
            <span class="text-brand-grey block text-sm leading-tight"
              >Strategic gateway to Eastern & Western Europe.</span
            >
          </div>
        </div>
      </div>
    </section>
    <section id="contact" class="relative px-6 py-24 md:px-12 md:py-32">
      <div class="absolute top-0 left-0 -z-10 h-full w-full overflow-hidden">
        <div
          class="bg-primary/10 absolute top-1/4 left-0 h-96 w-96 rounded-full
            blur-[100px]"
        ></div>
      </div>

      <div class="mx-auto max-w-7xl">
        <div class="mb-16">
          <h2 class="text-text_color text-4xl font-bold md:text-5xl">
            Get in touch
          </h2>
        </div>

        <div class="grid grid-cols-1 gap-4 lg:grid-cols-3">
          <div class="grid grid-cols-2 gap-4 lg:col-span-1 lg:flex lg:flex-col">
            <a
              href="tel:+36205154060"
              class="group relative flex flex-col justify-between
                overflow-hidden rounded-3xl bg-teal-900 p-6 text-white shadow-lg
                transition-transform duration-300 hover:scale-[1.01] sm:p-8"
            >
              <div class="mb-6 flex items-center justify-between">
                <span
                  class="rounded-full bg-white/20 p-2 backdrop-blur-md sm:p-3"
                >
                  <svg
                    class="size-5 text-white sm:size-6"
                    fill="none"
                    stroke="currentColor"
                    viewBox="0 0 24 24"
                  >
                    <path
                      stroke-linecap="round"
                      stroke-linejoin="round"
                      stroke-width="2"
                      d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z"
                    />
                  </svg>
                </span>
                <svg
                  class="hidden size-5 text-white/50 transition-transform
                    group-hover:translate-x-1 group-hover:-translate-y-1
                    sm:block"
                  fill="none"
                  stroke="currentColor"
                  viewBox="0 0 24 24"
                >
                  <path
                    stroke-linecap="round"
                    stroke-linejoin="round"
                    stroke-width="2"
                    d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"
                  />
                </svg>
              </div>
              <div class="flex flex-col items-start">
                <p
                  class="text-[10px] font-bold tracking-widest text-teal-300
                    uppercase sm:text-xs"
                >
                  Call
                </p>
                <p class="mt-1 text-sm font-bold sm:mt-2 sm:text-2xl">
                  +36 20 515 4060
                </p>
              </div>
            </a>

            <a
              href="mailto:gergo.szell@windeurotrans.com"
              class="group flex flex-col justify-between rounded-3xl border
                border-stone-500/10 bg-white/40 p-6 backdrop-blur-md
                transition-all hover:bg-white/60 sm:p-8"
            >
              <div class="mb-4 text-teal-800">
                <svg
                  class="size-7 sm:size-8"
                  fill="none"
                  stroke="currentColor"
                  viewBox="0 0 24 24"
                >
                  <path
                    stroke-linecap="round"
                    stroke-linejoin="round"
                    stroke-width="2"
                    d="M3 8l7.89 5.26a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z"
                  />
                </svg>
              </div>
              <div>
                <p
                  class="text-[10px] font-bold tracking-widest text-gray-400
                    uppercase sm:text-xs"
                >
                  Email
                </p>
                <p
                  class="text-text_color mt-1 text-xs font-semibold break-all
                    sm:text-base"
                >
                  gergo.szell@windeurotrans.com
                </p>
              </div>
            </a>

            <div
              class="flex flex-col justify-between rounded-3xl border
                border-stone-500/10 bg-white/40 p-6 backdrop-blur-md
                transition-all hover:bg-white/60 sm:p-8"
            >
              <div class="mb-4 text-teal-800">
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  width="24"
                  height="24"
                  viewBox="0 0 24 24"
                  fill="none"
                  stroke="currentColor"
                  stroke-width="2"
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  class="size-7 sm:size-8"
                >
                  <path
                    d="M6 18H4a2 2 0 0 1-2-2v-5a2 2 0 0 1 2-2h16a2 2 0 0 1 2 2v5a2 2 0 0 1-2 2h-2"
                  />
                  <path d="M6 9V3a1 1 0 0 1 1-1h10a1 1 0 0 1 1 1v6" />
                  <rect x="6" y="14" width="12" height="8" rx="1" />
                </svg>
              </div>
              <div>
                <p
                  class="text-[10px] font-bold tracking-widest text-gray-400
                    uppercase sm:text-xs"
                >
                  Fax
                </p>
                <p
                  class="text-text_color mt-1 text-xs font-semibold
                    sm:text-base"
                >
                  +36 1 700 2219
                </p>
              </div>
            </div>

            <div
              class="flex flex-col justify-between rounded-3xl border
                border-stone-500/10 bg-white/40 p-6 backdrop-blur-md
                transition-all hover:bg-white/60 sm:p-8"
            >
              <div class="mb-4 text-teal-800">
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  width="24"
                  height="24"
                  viewBox="0 0 24 24"
                  fill="none"
                  stroke="currentColor"
                  stroke-width="2"
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  class="size-7 sm:size-8"
                >
                  <circle cx="12" cy="12" r="10" />
                  <polyline points="12 6 12 12 16 14" />
                </svg>
              </div>
              <div>
                <p
                  class="text-[10px] font-bold tracking-widest text-gray-400
                    uppercase sm:text-xs"
                >
                  Business Hours
                </p>
                <p
                  class="text-text_color mt-1 text-xs font-semibold
                    sm:text-base"
                >
                  Monday - Friday: 8:00 - 17:00
                </p>
              </div>
